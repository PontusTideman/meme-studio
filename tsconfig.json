{
  "extends": "@viclafouch/eslint-config-viclafouch/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@viclafouch/meme-studio-utilities/*": ["node_modules/@viclafouch/meme-studio-utilities/dist/*"],
      "@stores/*": ["stores/*"],
      "@shared/*": ["shared/*"],
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@studio/*": ["modules/Studio/*"],
      "@models/*": ["models/*"],
      "@styled-system/*": ["../styled-system/*"],
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
