{
  "name": "meme-studio",
  "version": "1.0.0",
  "description": "A simple meme generator building in React",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --host 10.0.11.72",
    "commit": "git-cz",
    "build": "webpack --mode production",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "postinstall": "npx commitizen init cz-conventional-changelog --save-dev --save-exact --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/meme-studio.git"
  },
  "keywords": [
    "typescript",
    "react-hooks",
    "meme-generator"
  ],
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/meme-studio/issues"
  },
  "homepage": "https://github.com/viclafouch/meme-studio#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "babel-eslint": "10.0.3",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.3",
    "webpack-fix-style-only-entries": "0.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@types/react-color": "3.0.1",
    "@types/react-router-dom": "5.1.3",
    "abort-controller": "3.0.0",
    "i18next": "19.1.0",
    "i18next-browser-languagedetector": "4.0.1",
    "i18next-xhr-backend": "3.2.2",
    "immer": "5.3.4",
    "react": "16.12.0",
    "react-color": "2.18.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.3.1",
    "react-router-dom": "5.1.2",
    "react-svg": "11.0.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
