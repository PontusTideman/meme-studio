{
  "name": "meme-studio",
  "version": "1.0.0",
  "description": "A simple meme generator building in React",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "commit": "git-cz",
    "build": "webpack --mode production",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "postinstall": "npx commitizen init cz-conventional-changelog --save-dev --save-exact --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/meme-studio.git"
  },
  "keywords": [
    "typescript",
    "react-hooks",
    "meme-generator"
  ],
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/meme-studio/issues"
  },
  "homepage": "https://github.com/viclafouch/meme-studio#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "babel-eslint": "10.0.3",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.0.6",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.2",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-fix-style-only-entries": "0.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@types/react-color": "3.0.1",
    "abort-controller": "3.0.0",
    "react": "16.12.0",
    "react-color": "2.18.0",
    "react-dom": "16.12.0",
    "react-svg": "11.0.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
